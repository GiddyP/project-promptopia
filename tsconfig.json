{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@*": ["./*"]
    },
    "types": [
      "react-toastify"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "pages/_app.tsx",
    "sanity.ts",
    "components/global/Navbar/index.tsx",
    "pages/post/[slug].tsx",
    "pages/login/index.tsx",
    "pages/index.tsx"
, "components/Provider.tsx", "app/layout.tsx", "components/Nav.tsx", "next.config.js", "tailwind.config.js", "postcss.config.js"  ],
  "exclude": [
    "node_modules"
  ]
}